{
  "name": "react-sandbox",
  "version": "0.0.0",
  "private": true,
  "dependencies": {
    "@types/faker": "^5.5.8",
    "@types/uuid": "^8.3.1",
    "classnames": "^2.3.1",
    "faker": "^5.5.3",
    "fp-ts": "^2.11.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.0",
    "resize-observer-polyfill": "^1.5.1",
    "styled-components": "^5.3.0",
    "uuid": "^8.3.2"
  },
  "homepage": ".",
  "scripts": {
    "dev": "DISABLE_ESLINT_PLUGIN=true react-app-rewired start",
    "lint:tsx": "lint-staged",
    "lint:styled": "stylelint './src/**/*.styled.ts' --allow-empty-input",
    "lint:css": "prettier --write \"./src/**/*.{styled.ts,css}\" --no-error-on-unmatched-pattern",
    "lint": "npm run lint:tsx && npm run lint:styled && npm run lint:css",
    "postinstall": "husky install"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint . --fix",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.3.0",
    "@types/styled-components": "^5.1.12",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "chromatic": "^5.9.2",
    "dotenv": "^8.2.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.2",
    "prettier": "2.3.2",
    "react-app-rewire-alias": "^1.0.3",
    "react-app-rewired": "^2.1.8",
    "react-scripts": "4.0.3",
    "shelljs": "^0.8.4",
    "stylelint": "^13.13.1",
    "stylelint-config-recommended": "^5.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "typescript": "^4.1.2"
  }
}
